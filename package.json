    "lint-staged": "^15.2.5",
    "prettier": "3.2.5",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.11.0",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^0.20.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{js,ts}": "eslint"
